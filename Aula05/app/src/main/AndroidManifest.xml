<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="embedded.capacitacao.com.aula05">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>

            </intent-filter>
        </activity>
        <activity android:name=".SharedPreferencesActivity" android:label="Shared"/>
        <activity android:name=".StorageActivity" android:label="Storage" />
        <activity android:name=".SQLiteActivity" android:label="SQLite" />
        <activity android:name=".AddUserActivity" android:label="Add User" />

    </application>

</manifest>
